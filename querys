DROP TABLE registroacceso;

CREATE TABLE registroacceso (`isAccesoRegistro` int AUTO_INCREMENT PRIMARY KEY,`idPersona` int,`idUn` int,`fecha` date,`hora` time,`status` int,`carril` int,`idSentido` int,`mensaje` VARCHAR(255),`idEmpleado` int,`idTipoRegistroAcceso` int);

describe registroacceso;

